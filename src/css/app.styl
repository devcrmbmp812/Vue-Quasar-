@import '~variables'
@import url('https://fonts.googleapis.com/css?family=Open+Sans');
@import url('https://fonts.googleapis.com/css?family=Lato');

// app global css
html, body
  font-family "Open Sans" !important
p
  margin: 0
  //overflow-wrap: break-word
ul
  padding: 0
  margin: 0
li
  list-style: none
a
  text-decoration: none


//--- Overwrite Quasar Css Class ---//
.gutter-x-lg, .gutter-lg
  margin-left: 0
.gutter-y-sm, .gutter-sm
  margin-top: 0
.q-btn
  padding: 0
  min-height: 0
  text-transform none
  box-shadow: none
  border-radius 0
.q-datetime
  .q-btn
    padding: 4px 16px
    min-height: fit-content
    text-transform: uppercase
    .q-btn-inner
      font-size: 14px
      line-height: inherit
  .q-datetime-btn.active
    .q-btn-inner
      font-size: 26px
      line-height: 36px
.q-list
  border: none
  padding: 0
  .q-item
    padding: 0 12px
.q-checkbox-icon
  height: 44px
  width: 44px
  font-size: 44px
.q-if-standard
  width: 80%
  padding-bottom: 10px
.btn-toggle-standard
  display: block !important
  box-shadow: none
  margin-right 10px
  .q-btn
    background-color: $color-primary
    padding: 12px 22px
    margin-top: 12px
    margin-right: 20px
    margin-bottom: 12px
    .q-btn-inner
      //height: 36px
      line-height: 36px
      color: $color-standard
      font-size: 26px
  .q-btn.text-white
    .q-btn-inner
      color: $white
  .bg-primary
    background-color: $color-standard !important


//--- General Helper Class ---//
.container
  width: 86%
  margin 20px auto

.clear-fix
  clear: both
  content: ""
  display: table
.underline
  text-decoration: underline

.margin-top-main
  margin-top: 42px !important
.margin-top-13
  margin-top: 13px !important
.margin-top-15
  margin-top 15px !important
.margin-top-24
  margin-top: 24px !important
.margin-top-30
  margin-top: 30px !important
.margin-top-41
  margin-top 41px
.margin-top-44
  margin-top: 44px !important
.margin-bottom-main
  margin-bottom: 38px !important
.margin-bottom-128
  margin-bottom: 128px !important
.margin-left-34
  margin-left: 34px !important
.margin-right-20
  margin-right: 20px !important
.margin-right-30
  margin-right: 30px !important

.padding-horizontal-main
  padding-left: 10px !important
  padding-right: 10px !important

.align-left
  text-align left !important
.align-right
  text-align: right !important
.align-center
  text-align center !important


.input-wrapper-box
  position relative
  margin-top 15px
  padding-bottom 5px
  border-bottom 1px solid $color-primary

.checkbox-wrapper
  margin-top: 46px
  .check-box-item
    margin-top: 3px
    p
      //margin-left 2%
      margin-left: 20px
  .text-faded
    color: #C6DFF1!important

.img-icon-check-wrapper
  width: 24px
  height: 24px
  background-color: $color-standard
  border-radius: 50%
  img
    width: 50%
.img-icon-feature-wrapper
  width: 70px
  height: 70px
  border-radius: 50%
.img-icon-feature-wrapper.feature-one
  background-color: #DD8D09
.img-icon-feature-wrapper.feature-two
  background-color: #56BF2B
.img-icon-feature-wrapper.feature-three
  background-color: #B7AA08

.img-icon-check-medium-wrapper
  width: 50px
  height: 50px
  background-color: #56BF2B
  border-radius: 50%
  border-color: #1976d2
.img-icon-car-wrapper
  width: 40px
  height: 40px
  background-color: $color-standard
  border-radius: 50%


.select-common
  .q-input-target
    margin-left: 25px
  .q-if-addon
    position: absolute;
    left: 10px;
    font-size: 23px;
    color: $color-standard
  .row.no-wrap.relative-position
    align-items: center
.select-common.q-if-inverted
  height: 60px !important
  border: 1px solid $color-primary !important
  background-color: #F7F9FD !important
  margin-right:10px
  margin-top: 10px
  padding-left: 0
  padding-right: 0
  border-radius: 0
  box-shadow: none
  .q-input-target
    height: 36px
    line-height: 36px
    color: $color-standard
    font-size: 26px
    margin: 12px 22px

//--- Colors ----//
.white
  color: $white !important
.color-standard
  color: $color-standard !important
.bg-color-main
  background: $color-standard !important
.bg-color-primary
  background: linear-gradient(90deg, #D28608 0%, #FBA516 100%) !important
.bg-color-secondary
  background: linear-gradient(90deg, #2B7DBF 0%, #4BC6DF 100%) !important
.bg-color-customer-main
  background: #F6F6F6
.bg-color-customer-primary
  background: #2E2E2E !important

.alpha-main
  opacity: 0.25


//--- Fonts ---//
.font-family-lato
  font-family: Lato

.text-font-title, .text-font-1, .text-font-3, .text-font-4,
.text-font-5, .text-font-6, .text-font-7, .text-font-12, .text-font-15
  color: $color-standard
.text-font-main, .text-font-8, .text-font-9, .text-font-10, .text-font-11,
.text-font-13, .text-font-14, .text-font-17, .text-font-18, .text-font-19,
.text-font-21
  color: $color-main
.text-font-2
  color: #969696
.text-font-16
  color: #609ECF

.text-font-title, .text-font-1, .text-font-5, .text-font-6,
.text-font-7, .text-font-8, .text-font-11, .text-font-13,
.text-font-14, .text-font-19, .text-font-20
  font-weight: $font-weight-regular
.text-font-main, .text-font-2, .text-font-4, .text-font-9, .text-font-15
.text-font-17, .text-font-18, .text-font-21
  font-weight: $font-weight-light

.text-font-main, .text-font-1, .text-font-4, .text-font-8
.text-font-19
  line-height: 36px
.text-font-20
  line-height: 40px
.text-font-21
  line-height: 20px

.text-font-main, .text-font-1, .text-font-4, .text-font-8
  font-size: 24px
.text-font-title, .text-font-13
  font-size: 32px
.text-font-2, .text-font-9
  font-size: 20px
.text-font-3, .text-font-7, .text-font-10, .text-font-15
  font-size: 14px
.text-font-5
  font-size: 44px
.text-font-6
  font-size: 30px
.text-font-11
  font-size: 16px
.text-font-12
  font-size: 26px
.text-font-14
  font-size: 44px
.text-font-16, .text-font-18, .text-font-20
  font-size: 18px
.text-font-17
  font-size: 22px
.text-font-18
  line-height: 30px
.text-font-19
  font-size: 36px
.text-font-21
  font-size: 12px


.font-weight-none
  font-weight: unset !important
.font-weight-light
  font-weight: 300 !important
.font-weight-regular
  font-weight: 700 !important
.font-weight-bold
  font-weight: bold !important


//--- Image Icon ---//
.img-icon-star:after
  content:url('~assets/img/icon-star.png')
