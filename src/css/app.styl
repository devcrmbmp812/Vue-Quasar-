//---- Page Header Begin ----//
.page-header-box
  height: 103px
  padding: 0 4%
  background: linear-gradient(180deg, #45667C 0%, #213546 100%);
  .q-tabs-head
    height: 103px
    background: transparent !important
    .q-link.q-tab.active
      .q-tab-label
        color: #3F8BA1
    .q-tabs-bar
      left: inherit
      right: inherit
      width: 50px
      border-bottom-width: 4px
  .navigation-box
    .nav-page-name
      margin-right: 304px
    .header-action-btn-box
      position:absolute
      top: 28px


.account-box, .live-chat-box
  border-radius: 27px
  padding 7px 20px
//box-shadow: 0 20px 50px 0 #CBCBCB;
.account-box
  width: 144px
  background-color: $color-standard
  .separator
    height: 1px
    background-color: #FFFFFF
    opacity: 0.2
    margin-top: 5px
    margin-bottom: 10px
  .drop-down
    color: #FFFFFF
  .log-out
    margin-bottom: 6px
.live-chat-box
  width: 120px
  height: 45px
  padding 7px 16px
  background: linear-gradient(90deg, #D28608 0%, #FBA516 100%)
.account-text
  display: block
  color: $white
  font-family: Lato
  font-size: 16px
  font-weight: bold
  height: 32px
  line-height: 32px
.account-text:hover, .drop-down:hover
  cursor: pointer

.toolbar-menu-btn
  .q-icon
    font-size: 40px


//--- Page Footer ---//
.page-footer-separator
  height: 30px
  background-color: $color-standard
.page-footer-content
  //height: 370px
  padding: 49px 7vw
  background-color: $color-main
  .useful-link
    margin-bottom 50px
  .sign-up-box
    .description
      width: 363px
      height 79px
      line-height: 24px
      opacity: 0.25
    .label
      height: 48px
      line-height: 48px
      font-size: 12px
      font-weight: bold
      opacity: 0.25
    .btn-sign-up
      width: 149px
      .q-btn-inner
        font-family: Lato
        font-size: 22px
        font-weight: 900
        line-height: 27px
    .q-input-target
      color: #FFFFFF
      font-size: 24px
      font-weight: 300
      height: 48px
      line-height: 48px
    .q-if-standard
      padding: 0
    .separator
      height: 4px
      margin-bottom: 30px
      border-bottom: 1px solid #FFFFFF
      border-right: 1px solid #FFFFFF
      opacity: 0.25

.page-footer-end
  //height: 85px
  padding: 31px 7vw 32px 7vw
  background-color: #636363


//--- Page Fixed Button Box ---//
.fixed-button-box
  .btn-prev, .btn-next
    background-color: $color-standard
    border-radius: 0
  .btn-prev.hidden, .btn-next.hidden
    display block  !important
    background-color: $color-primary
  //cursor not-allowed
  .btn-general-help
    background-color: $color-main
    border-radius: 0


//--- General Help Box ---//
.general-help-box
  opacity: 0.97
  background: $color-main
  .btn-wrapper-box
    margin: 30px
  .btn-wrapper-box.btn-close
    background: $white
  .help-content-box
    .help-caption
      color: $white
      font-size: 28px
      font-weight: bold
      line-height: 32px
      padding-bottom: 17px
      border-bottom: 1px solid #636363
      margin-top: 75px
    .help-content
      color: #EAEAEA
      font-size: 20px
      font-weight: $font-weight-light
      line-height: 34px
      margin-top 35px
    .btn-link
      margin-top: 53px
      padding: 12px 28px
      background: linear-gradient(90deg, $color-standard 0%, #4BC6DF 100%)
      //box-shadow: 0 20px 50px 0 #CBCBCB
      border-radius: 27px;
      color: $white
      font-family: Lato
      font-size: 20px
      font-weight: bold
      text-align: center


//--- Left Drawer ---//
.q-layout-drawer
  .q-list-header
    height 100px
  .q-item
    text-align: center
    height 60px


.btn-main
  height: 60px
  border-radius: 0
.btn-round-first, .btn-round-second
  border-radius: 27px
  //line-height: 32px
  text-transform: uppercase
  box-shadow: 0 20px 50px 0 #CBCBCB
  .q-btn-inner
    font-family: Lato
    font-weight: bold
    color: $white
    //line-height: 32px
.btn-round-first
  background: linear-gradient(90deg, #2B7DBF 0%, #4BC6DF 100%)
.btn-round-second
  background: linear-gradient(90deg, #D28608 0%, #FBA516 100%)
.title-box-main
  height: 60px
  padding: 0 30px
  background-color: $color-standard
  .title-text-main
    height: 29px
    line-height: 29px
    font-family: Lato
    font-size: 24px
    color: $white
.btn-nav, .btn-standard
  padding:12px 22px
  margin-top: 12px
  margin-bottom: 12px
  border-radius: 0
.q-btn-inner
  line-height: 36px
  font-size: 26px
.btn-nav
  background: linear-gradient(90deg, #D28608 0%, #FBA516 100%)
  .q-btn-inner
    color: $white
.btn-standard
  background-color: $color-primary
  .q-btn-inner
    color: $color-standard
.btn-standard.selected
  background-color: $color-standard
  .q-btn-inner
    color: #FFFFFF


@media screen and (max-width: 1120px)
  .toolbar-action-box
    display: none
@media screen and (min-width: 1120px)
  .toolbar-menu-btn
    display: none
@media screen and (min-width: 1800px)
  .q-layout-page
    max-width 1800px !important
    margin 0 auto !important


//Prometheus added
.nav-top
  height: 150px
  width: 100%
  position: absolute
  top: 0
  left: 0
.logo-box-alpha
  opacity: 0.9!important

.img-icon-logo-main-background
  position: relative
  top: 50%
  left: 50%
  -webkit-transform: translate(-50%, -50%)
  -moz-transform: translate(-50%, -50%)
  -ms-transform: translate(-50%, -50%)
  -o-transform: translate(-50%, -50%)
  transform: translate(-50%, -50%)

.img-icon-logo-main-background:after
  position: relative
  top: calc(50% - 35px)
  left: calc(50% - 69px)
  -webkit-transform: translate(-50%, -50%)
  -moz-transform: translate(-50%, -50%)
  -ms-transform: translate(-50%, -50%)
  -o-transform: translate(-50%, -50%)
  transform: translate(-50%, -50%)
  content:url('~assets/img/logo-yoga.png')

.side-bar-container {
  width 100%
}
.validation-error-label {
  padding 0 11px
  background-color red
  color #ffffff
  font-weight bold
  font-size 20px;
  margin-top 5px
  border-radius 50%
}
.question-title {
  float: left
  margin-right 10px
}
.d-flex {
  display flex
}
.text-font-main .validation-error-label {
  line-height initial!important
}
